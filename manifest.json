{

    "manifest_version": 2,
    "name": "Background Chager",
    "description": "Add a Custom Background",
    "version": "1.0",
    "icons": {
        "16": "/images/icons/image_16.png",
        "48": "/images/icons/image_48.png",
        "128": "/images/icons/image_128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/icons/image_16.png",
            "48": "/images/icons/image_48.png",
            "128": "/images/icons/image_128.png"
        }
    },
    "content_scripts": [{
        "matches":["https://www.facebook.com/*"],
        "css": ["main.css"]
    }]

}